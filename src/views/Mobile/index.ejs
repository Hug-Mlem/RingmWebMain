    <style amp-boilerplate>
        body {
            -webkit-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
            -moz-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
            -ms-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
            animation: -amp-start 8s steps(1, end) 0s 1 normal both
        }

        @-webkit-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @-moz-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @-ms-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @-o-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }
    </style><noscript>
        <style amp-boilerplate>
            body {
                -webkit-animation: none;
                -moz-animation: none;
                -ms-animation: none;
                animation: none
            }
        </style>
    </noscript>
    <style amp-custom>
        /* header */

        header {
            background-color: #ffffff;
            position: fixed;
            top: 0;
            width: 90%;
            z-index: 1;
            display: block;
            padding-left: 5%;
            padding-right: 5%;
            padding-top: 0.5%;
            padding-bottom: 1.5%;
        }

        header .icon1 {
            padding-top: 2%;
            width: 10%;
            float: left;
        }

        header .icon2 {
            padding-top: 2%;
            width: 10%;
            float: left;
        }

        header .logo {
            padding-top: 2%;
            width: 60%;
            float: left;
        }

        header .logo amp-img {
            margin-left: auto;
            margin-right: auto;
            width: 70%;
        }

        header .collap {
            padding-top: 3%;
            width: 20%;
            float: left;
        }

        header .collap button.on {
            float: right;
            margin-left: auto;
            margin-right: auto;
            border: 0px;
            font-size: 6vw;
            background-color: #ffffff;
            height: auto;
            max-width: 100%;
            display: block;
        }

        header .collap div button.off {
            border: 0px;
            font-size: 30px;
            background-color: #efefef;
            display: block;
            float: left;
            padding-left: 20px;
        }

        header .collap div {
            background-color: #efefef;
            width: 100%;
            height: 5%;
            display: block;
            text-align: right;
        }

        .amp-carousel-button {
            background-color: rgba(0, 0, 0, 0);
        }


        /*Resize cho màn ipad */
        @media screen and (min-width:768px) {
            header .icon2 {
                margin-left: -3%;
                margin-right: 3%;
            }

            header .logo {
                padding-top: 1%;
            }

            header .logo amp-img {
                width: 48%;
                min-height: 51px;
            }

            header .collap {
                padding-top: 0.5%;
            }
        }

        /*----------------*/

        /* side bar */
        amp-sidebar {
            max-width: 200vw;
        }

        section::after {
            display: table;
            clear: both;
        }

        .sample-sidebar {
            width: 100%;
            font-size: 4vw;
        }

        .sample-sidebar ul {
            padding: 10px;
        }

        nav[toolbar] li {
            display: block;
            padding: 20px;
            margin: var(--space-2);
            margin-left: var(--space-4);
            list-style: none;
        }

        .sample-sidebar li span {
            text-align: right;
            float: right;
            width: 10%;
            display: block;
            font-size: 5.5vw;
        }

        .sample-sidebar li.onPage {
            background-color: #0086ED;
        }

        header .collap div#dialog {
            display: none;
        }

        .sample-sidebar li.onPage strong {
            color: #ffffff;
        }

        .amp-sidebar-mask {
            background-color: rgba(61, 54, 54, 0.219);
            z-index: -1;
        }

        .sample-sidebar amp-mega-menu .megaMenu {
            text-align: center;
        }

        .sample-sidebar amp-mega-menu span {
            float: right;
        }

        /*----------------*/
        a {
            display: block;
            width: 100%;
            text-decoration: none;
            color: unset;
            background-color: transparent;
        }

        body * {
            cursor: default;
            font-family: 'Roboto', sans-serif;
            outline: 0px;
            color: #333;
            font-size: 1rem;
        }


        #target-element-left {
            display: none;
        }



        .banner {
            margin-bottom: -2.5rem;
        }

        .amp-carousel-button-next,
        .amp-carousel-button-prev {
            opacity: 1 !important;
            z-index: 0;
            height: 13px;
            width: 13px;
            pointer-events: auto !important;
        }

        i-amphtml-sizer {
            padding-top: 50% !important;
        }

        .text {
            padding-right: 15px;
            padding-left: 15px;
        }

        .text .title {
            color: #404066;
            font-size: 1.4rem;
            font-weight: 700;
            line-height: 1.4;

        }

        .text .title:after {
            content: '';
            width: 5rem;
            height: 0.1rem;
            background-color: #0086ED;
            position: absolute;
            bottom: 22%;
            left: 15px;
        }



        .text .descr {
            font-size: 0.44rem;
            color: #404066;
        }

        @media screen and (max-width:991px) {


            .banner {
                margin-bottom: -3rem;
            }

            .amp-carousel-button-next,
            .amp-carousel-button-prev {

                height: 50px;
                width: 50px;

            }

            i-amphtml-sizer {
                padding-top: 42% !important;
            }

            .text {
                width: 60%;
                float: left;
            }

            .pic {
                width: 40%;
                float: left;
            }

            .text .title {
                color: #404066;
                font-size: 2.2rem;
            }

            .text .title:after {
                width: 8rem;
                height: 0.2rem;
                bottom: -12%;
            }

        }
    </style>

</head>

<body>

